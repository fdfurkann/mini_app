// Original function renamed
function useLocaleHook() {
  const { lang } = useLang();
  return (key: string) => {
    // Ensure translations[lang] or translations['tr'] exists before accessing [key]
    const currentTranslations = translations[lang] || translations['tr'];
    return currentTranslations?.[key] || key; // Simplified fallback
  };
}

export const useT = useLocaleHook; // Exporting with the original name 'useT'

export const locales = {
  tr: translations.tr,
  en: translations.en,
  de: translations.de,
  it: translations.it,
  ru: translations.ru,
  ar: translations.ar,
  ja: translations.ja
};

export const t = (key: string, lang: string = 'tr'): string => {
  const translations = locales[lang] || locales['tr'];
  return translations[key] || key;
}; 